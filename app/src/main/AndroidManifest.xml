<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="redtoss.example.furstychristmas">

    <application
        android:allowBackup="true"
        android:icon="@drawable/custom_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/custom_logo_rounded"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="redtoss.example.furstychristmas.MainActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="redtoss.example.furstychristmas.eula.EulaActivity"
            android:label="eula" />

        <receiver android:name="redtoss.example.furstychristmas.receiver.DailyNotificationReceiver" />
    </application>

</manifest>